file = {:C$\backslash$:/Users/cadur/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Su'ait et al. - 2012 - Preparation and characterization of blended solid polymer electrolyte 49{\%} poly(methyl methacrylate)-grafted natur.pdf:pdf},
issn = {14328488},
journal = {J. Solid State Electrochem.},
keywords = {49{\%}Poly(methyl methacrylate)-grafted natural rubbe,Ionic conductivity,Lithiumtetrafluoroborate (LiBF 4),Poly(methyl methacrylate) (PMMA),Solid polymer electrolyte},
number = {6},
pages = {2275--2282},
title = {{Preparation and characterization of blended solid polymer electrolyte 49{\%} poly(methyl methacrylate)-grafted natural rubber:poly(methyl methacrylate)-lithium tetrafluoroborate}},
volume = {16},
