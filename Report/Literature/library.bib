file = {:C$\backslash$:/Users/cadur/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ryba, Krňoul - 2019 - Segmentation of shoulder MRI data for musculoskeletal model adaptation.pdf:pdf},
